generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phoneNo   String
  age       String
  answers   String[] @default([])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
